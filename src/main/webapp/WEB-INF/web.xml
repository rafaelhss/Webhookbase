<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>bot</servlet-name>
        <servlet-class>bot.BotServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>bot</servlet-name>
        <url-pattern>/bot</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>botNumeros</servlet-name>
        <servlet-class>bot.BotServletNumeros</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>botNumeros</servlet-name>
        <url-pattern>/botNumeros</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>botComandos</servlet-name>
        <servlet-class>bot.BotServletComando</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>botComandos</servlet-name>
        <url-pattern>/botComandos</url-pattern>
    </servlet-mapping>
</web-app>
